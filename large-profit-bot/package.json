{
  "name": "large-profit-bot",
  "version": "0.0.5",
  "description": "Forta bot that detects large profits",
  "repository": "https://github.com/NethermindEth/Forta-Agents/tree/main/large-profit-bot",
  "chainIds": [
    1,
    56,
    10,
    137,
    42161,
    250,
    43114
  ],
  "chainSettings": {
    "1": {
      "shards": 12,
      "target": 3
    },
    "56": {
      "shards": 23,
      "target": 3
    },
    "137": {
      "shards": 24,
      "target": 3
    },
    "43114": {
      "shards": 2,
      "target": 3
    },
    "10": {
      "shards": 6,
      "target": 3
    },
    "default": {
      "shards": 5,
      "target": 2
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,ts,json  --exec \"npm run build && forta-agent run\"",
    "start:prod": "forta-agent run --prod",
    "tx": "npm run build && forta-agent run --tx",
    "block": "npm run build && forta-agent run --block",
    "range": "npm run build && forta-agent run --range",
    "alert": "npm run build && forta-agent run --alert",
    "sequence": "npm run build && forta-agent run --sequence",
    "file": "npm run build && forta-agent run --file",
    "publish": "forta-agent publish",
    "info": "forta-agent info",
    "logs": "forta-agent logs",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest --detectOpenHandles",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "forta-agent": "^0.1.31",
    "lru-cache": "^7.14.1",
    "node-fetch": "^2.6.8",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemon": "^1.19.0",
    "@types/jest-when": "^3.5.2",
    "jest": "^27.0.6",
    "nodemon": "^2.0.8",
    "prettier": "^2.8.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4",
    "forta-agent-tools": "^3.2.5",
    "jest-when": "^3.5.2"
  }
}
